{
  "name": "survey-gorilla-api",
  "version": "1.0.0",
  "description": "POC API for a survey service",
  "main": "index.ts",
  "author": "Ryan Brandt",
  "license": "MIT",
  "scripts": {
    "start:hot": "nodemon",
    "start": "ts-node ./src/index.ts",
    "build": "tsc",
    "knex": "knex --knexfile=src/db/config/knexfile.ts",
    "db:migrate-up": "yarn knex -- migrate:latest",
    "db:migrate-down": "yarn knex -- migrate:down",
    "db:migrate-create": "yarn knex -- migrate:make -x ts"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/express": "^4.17.13",
    "@types/knex": "^0.16.1",
    "@types/node": "^16.4.7",
    "@types/pg": "^8.6.1",
    "nodemon": "^2.0.12",
    "ts-node": "^10.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "knex": "^0.95.8",
    "objection": "^2.2.15",
    "pg": "^8.7.1"
  }
}
