{
  "name": "survey-gorilla-api",
  "version": "1.0.0",
  "description": "POC API for a survey service",
  "main": "index.ts",
  "author": "Ryan Brandt",
  "license": "MIT",
  "scripts": {
    "start:hot": "nodemon -L",
    "start": "ts-node ./src/index.ts",
    "build": "tsc",
    "test": "jest",
    "knex": "knex --knexfile=src/db/config/knexfile.ts",
    "db:migrate-up": "yarn knex migrate:latest",
    "db:migrate-down": "yarn knex migrate:down",
    "db:migrate-create": "yarn knex migrate:make -x ts"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/knex": "^0.16.1",
    "@types/node": "^16.4.7",
    "@types/pg": "^8.6.1",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.0.6",
    "nodemon": "^2.0.12",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.4",
    "ts-node": "^10.1.0",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-validator": "^6.12.1",
    "knex": "^0.95.8",
    "objection": "^2.2.15",
    "pg": "^8.7.1"
  }
}
